{
  "name": "stom-labs",
  "version": "0.0.1",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "build"
  },
  "build": {
    "productName": "Head and neck anatomy",
    "copyright": "Copyright Â© 2018 Alex Glinsky",
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "files": [
      "node_modules/**/*",
      "package.json",
      "*.json",
      "*.js",
      "*.html",
      "css/*",
      "images/*",
      "js/*",
      "db.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "extraFiles": [
      "db.json"
    ],
    "extraResources": [
      "db.json"
    ]
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^2.0.2",
    "electron-builder": "^20.26.1"
  },
  "dependencies": {
    "lowdb": "^1.0.0",
    "materialize-css": "^1.0.0-rc.1"
  }
}
